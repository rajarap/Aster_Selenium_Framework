<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="1Aster_Web_Regression_Test_Suite" parallel="tests" thread-count="4">
	<listeners>
		<!--<listener class-name="com.web.aster.Listeners.TestListener" />-->
		<listener class-name="org.uncommons.reportng.HTMLReporter" />
	</listeners>

	<test name="HomePageVerification_01">
		<parameter name="browser" value="chrome" />
		<parameter name="version" value="null" />
		<parameter name="environment" value="uat" />
		<parameter name="platform" value="null" />
		<parameter name="executionEnv" value="local" />
		<classes>
			<class name="com.web.aster.Tests.HomePageVerification" />
		</classes>
	</test>
	

	<test name="HomePageVerification_02">
		<parameter name="browser" value="firefox" />
		<parameter name="version" value="null" />
		<parameter name="environment" value="uat" />
		<parameter name="platform" value="null" />
		<parameter name="executionEnv" value="local" />
		<classes>
			<class name="com.web.aster.Tests.HomePageVerification" />
		</classes>
	</test>
</suite>