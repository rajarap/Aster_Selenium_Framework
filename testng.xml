<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="1Aster_Web_Regression_Test_Suite" parallel="tests" thread-count="4">
	<listeners>
		<listener class-name="com.web.aster.Listeners.TestListener" />
		<listener class-name="org.uncommons.reportng.HTMLReporter" />
	</listeners>

	<test name="Regression_Chrome" group-by-instances="true">
		<parameter name="browser" value="chrome" />
		<parameter name="version" value="null" />
		<parameter name="environment" value="pre-prod" />
		<parameter name="platform" value="null" />
		<parameter name="executionEnv" value="local" />
		<classes>
			<class name="com.web.aster.Tests.TC002_Login_With_Email" />
			<class name="com.web.aster.Tests.TC003_Book_InPerson_Doctor_Appointment" />
		</classes>
	</test>
		
</suite>

